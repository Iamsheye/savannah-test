{
  "name": "aryon-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:auth": "concurrently \"bun run dev:server:auth\" \"bun run dev:client\"",
    "dev:client": "wait-on tcp:3001 && vite",
    "dev:server": "cd server && bun run dev",
    "dev:server:auth": "cd server && bun run dev:auth",
    "build": "bun run build:server && bun run build:client",
    "build:client": "tsc -b && vite build",
    "build:server": "cd server && bun run build",
    "install:all": "bun install && cd server && bun install",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:server": "cd server && bun test"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.61.5",
    "@tanstack/react-router": "^1.84.0",
    "axios": "^1.7.8",
    "concurrently": "^9.1.0",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.13.1",
    "wait-on": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tanstack/router-devtools": "^1.84.0",
    "@tanstack/router-plugin": "^1.83.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "2.1.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  }
}
